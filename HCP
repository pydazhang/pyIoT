sublist=["Extracellular region or secreted", "Cytosol", "Plasma membrane", "Cytoskeleton", "Lysosome", "Endosome", "Peroxisome", "Endoplasmic reticulum", "Golgi apparatus", "Nucleus", "Mitochondrion","cytoplasm","midbody","integral component of membrane","Cytoplasm","glial limiting end-foot","collagen trimer"]

for i in proteiname:

    link1 = "https://www.uniprot.org/uniprot/name".replace("name",i)
    url = link1
    html = urlopen(url)
    soup = BeautifulSoup(html, 'lxml')
    text = soup.get_text()
    mw = re.findall("Da.*Last", text)
    mw = mw[0].replace('Da):','').replace('Last','').replace(',','')
    nm = re.findall('_CRIGR .* OS=Cricetulus', text)
    nm = nm[0].replace('_CRIGR ', '').replace(' OS=Cricetulus', '')

    if re.findall('Cellular component', text):
        loc = []
        loca=''
        range = re.findall('Cellular component.*\n', text)
        for j in sublist:
            subname = re.search(j, range[0])
            if (subname):
                loc.append(j)
        for k in loc:
            loca= loca + k + ', '
    else:
        loca = 'N/A'

    link2 = "https://web.expasy.org/cgi-bin/compute_pi/pi_tool1?name@noft@average".replace("name", i)
    url = link2
    html = urlopen(url)
    soup = BeautifulSoup(html, 'lxml')
    text = soup.get_text()
    if re.search("pI: .*[0-9]", text):
        pi = re.findall("pI: .*[0-9]", text)
        pi = pi[0].replace('pI: ', '')
    else:
        pi= "N/A"
    f = open("log.txt", "a")
    f.write(nm + '('+i+ ') : ' +mw+' : '+pi+' : '+loca+'\n')
    f.close()

    print(nm + '('+i+ ') : ' +mw+' : '+pi+' : '+loca+'\n')